{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.RollingFile"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": ".\\Logs\\BusinessRegister-{Date}.txt"
        },
        "OutputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {SourceContext}{NewLine}{Message}{NewLine}in method {MemberName} at {FilePath}:{LineNumber}{NewLine}{Exception}{NewLine}"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "BusinessRegister"
    }
  },
  "ConnectionStrings": {
    "BusinessRegister": "Server=localhost\\EKTACO;Database=BusinessRegister2;user id=businessregister;password=Parool1234;"
  },
  "WebServerSettings": {
    "VirtualDir":  "/BusinessRegister"
  } 
}
